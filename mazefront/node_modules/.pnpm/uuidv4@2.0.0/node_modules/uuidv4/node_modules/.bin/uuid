#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/mazefront/node_modules/.pnpm/uuid@3.3.2/node_modules/uuid/bin/node_modules:/mnt/c/mazefront/node_modules/.pnpm/uuid@3.3.2/node_modules/uuid/node_modules:/mnt/c/mazefront/node_modules/.pnpm/uuid@3.3.2/node_modules:/mnt/c/mazefront/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/mazefront/node_modules/.pnpm/uuid@3.3.2/node_modules/uuid/bin/node_modules:/mnt/c/mazefront/node_modules/.pnpm/uuid@3.3.2/node_modules/uuid/node_modules:/mnt/c/mazefront/node_modules/.pnpm/uuid@3.3.2/node_modules:/mnt/c/mazefront/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uuid@3.3.2/node_modules/uuid/bin/uuid" "$@"
else
  exec node  "$basedir/../../../../../uuid@3.3.2/node_modules/uuid/bin/uuid" "$@"
fi
